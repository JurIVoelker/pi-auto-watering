{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prod:deploy": "npm run minio:import-migrations && npm run db:migrate && npm run minio:export-migrations && npm run db:default-plant && npm run build",
    "db:prod:migrate": "npx prisma migrate dev --name init --schema ./prisma/prod.prisma",
    "db:migrate": "npx prisma migrate dev --name init --schema ./prisma/dev.prisma",
    "db:create": "npx tsx ./prisma/scripts/setupDatabase.ts && npm run db:migrate",
    "db:default-plant": "npx tsx ./prisma/scripts/setupDefaultPlant.ts",
    "db:test-data": "npx tsx ./prisma/scripts/addTestData.ts",
    "db:test": "npx tsx ./prisma/scripts/testDatabase.ts",
    "minio:import-migrations": "npx tsx ./prisma/scripts/importMigrations.ts",
    "minio:export-migrations": "npx tsx ./prisma/scripts/uploadMigrations.ts"
  },
  "dependencies": {
    "@prisma/client": "6.5.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@types/unzipper": "^0.10.11",
    "archiver": "^7.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "image-size": "^2.0.1",
    "lucide-react": "^0.484.0",
    "minio": "^8.0.5",
    "next": "15.2.3",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.2",
    "tw-animate-css": "^1.2.4",
    "unzipper": "^0.12.3",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/archiver": "^6.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
